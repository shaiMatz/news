{
  "expo": {
    "name": "NewsGeo",
    "slug": "newsgeo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./generated-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./generated-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./generated-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./generated-icon.png",
      "bundler": "metro"
    },
    "extra": {
      "apiUrl": "https://${REPL_SLUG}.${REPL_OWNER}.repl.co/api"
    },
    "plugins": []
  }
}